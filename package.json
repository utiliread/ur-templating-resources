{
  "name": "ur-templating-resources",
  "version": "1.1.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json && rimraf dist/cjs/**/*.d.ts && copyfiles -u 1 src/**/*.{css,html} dist/cjs && copyfiles -u 1 src/**/*.{css,html} dist/esm",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/luxon": "^0.2.5",
    "@types/select2": "^4.0.41",
    "aurelia-framework": "^1.1.5",
    "copyfiles": "^1.2.0",
    "luxon": "^0.5.3",
    "rimraf": "^2.6.2",
    "typescript": "^2.7.2",
    "ur-humanize": "^1.0.3"
  },
  "peerDependencies": {
    "aurelia-framework": "^1.1.5",
    "luxon": "^0.5.3",
    "ur-humanize": "^1.0.3"
  },
  "dependencies": {
    "select2": "^4.0.6-rc.1"
  }
}
